  if (enabledHeater) {
    if (enabledPump) {
      switch(imageIndex) {
        case 1:
          u8g2.drawXBMP(0, 0, 128, 64, background_heater_pump_on_1);
          imageIndex++;
          break;
        case 2:
          u8g2.drawXBMP(0, 0, 128, 64, background_heater_pump_on_2);
          imageIndex++;
          break;
        case 3:
          u8g2.drawXBMP(0, 0, 128, 64, background_heater_pump_on_3);
          imageIndex++;
          break;
        case 4:
          u8g2.drawXBMP(0, 0, 128, 64, background_heater_pump_on_4);
          imageIndex = 1;
          break;
      }
    } else {
      u8g2.drawXBMP(0, 0, 128, 64, background_heater_pump_off);
    }
  } else {
    if (enabledPump) {
      switch(imageIndex) {
        case 1:
          u8g2.drawXBMP(0, 0, 128, 64, background_non_heater_pump_1);
          imageIndex++;
          break;
        case 2:
          u8g2.drawXBMP(0, 0, 128, 64, background_non_heater_pump_2);
          imageIndex++;
          break;
        case 3:
          u8g2.drawXBMP(0, 0, 128, 64, background_non_heater_pump_3);
          imageIndex++;
          break;
        case 4:
          u8g2.drawXBMP(0, 0, 128, 64, background_non_heater_pump_4);
          imageIndex = 1;
          break;
      }
    } else {
      u8g2.drawXBMP(0, 0, 128, 64, background_non_heater_pump_off);
    }
  }

  u8g2.setFont(u8g2_font_HelvetiPixelOutline_te);
  u8g2.setCursor(1, 2);
  u8g2.print(convertTemperature(temperatureHeater));
  u8g2.setCursor(1, 44);
  u8g2.print(convertTemperature(temperaturePool));


  u8g2.setFont(u8g2_font_t0_12_mr);
  u8g2.setCursor(101, 0);
  u8g2.print(String(targetTemperature, 1));
  
  u8g2.setCursor(96, 18);
  u8g2.print(String(dailyTempChange, 1));
  
  printDailyHeaterRuntime();
  printDailyPumpRuntime();
  calculateWeatherToday();
  
